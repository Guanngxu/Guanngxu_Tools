<view class="input-data">
  <t-input model:value="{{Vcs}}" placeholder="CS 引脚对地电阻电压测量值 Vcs" type="digit" tips="CS 引脚对地电阻电压测量值 Vcs，单位：V" />
  <t-input model:value="{{Rcs}}" placeholder="CS 引脚对地电阻 Rcs" type="digit" tips="CS 引脚对地电阻 Rcs，单位：kohm" />
</view>

<view class="compute-button">
  <t-button bind:tap="compute_click" size="large" theme="light" variant="outline" block>计算输出电流</t-button>
</view>
<view class="compute-result">
  <t-input disabled value="{{Iout}}" placeholder="输出电流 Iout" type="digit" tips="输出电流 Iout，单位：A" />
</view>